<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="125.0" prefWidth="385.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ru.mag.db.jdbc.controllers.AddNewManufacturerDialog">
   <opaqueInsets>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </opaqueInsets>
   <children>
      <Label layoutX="15.0" layoutY="14.0" prefHeight="25.0" prefWidth="150.0" text="New manufacturer name:" />
      <Label layoutX="15.0" layoutY="48.0" prefHeight="25.0" prefWidth="150.0" text="New manufacturer country:" />
      <TextField fx:id="txtNewManufacturerName" layoutX="165.0" layoutY="14.0" prefHeight="25.0" prefWidth="200.0" promptText="New manufacturer name..." />
      <TextField fx:id="txtNewManufacturerCountry" layoutX="165.0" layoutY="48.0" prefHeight="25.0" prefWidth="200.0" promptText="New manufacturer country..." />
      <Button fx:id="btnSaveNewManufacturer" layoutX="324.0" layoutY="83.0" mnemonicParsing="false" onAction="#btnSaveNewManufacturer_Click" text="Save" />
      <Button fx:id="btnCancelDialog" layoutX="262.0" layoutY="83.0" mnemonicParsing="false" onAction="#btnCancelDialog_Click" text="Cancel" />
   </children>
</AnchorPane>
